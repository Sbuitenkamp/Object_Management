@page
@model Object_management.Pages.Objects.AddType

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<partial name="_HeadPartial"/>
<script src="/js/user-feedback.js"></script>
<body>
<partial name="_MenuPartial"/>
<div class="content">
<a href="/objects" class="breadcrumb"><i class="fa fa-solid fa-arrow-left-long"></i> Terug naar fietsoverzicht</a>
    <h1 class="warning warning--hidden"></h1>
    <h1 class="confirmation confirmation--hidden"></h1>
    <h1>Fietssoort toevoegen</h1>
    <form class="content__form" method="post">
        <div class="content__form__row">
            <label class="content__form__row__label" for="description">Omschrijving</label>
            <input class="content__form__row__input" asp-for="Description">
        </div>
        <div class="content__form__row">
            <label class="content__form__row__label" for="price">Prijs</label>
            &euro;<input class="content__form__row__input" asp-for="Price">
        </div>
        <button type="submit" class="content__form__button button">Toevoegen</button>
    </form>
</div>
</body>
</html>